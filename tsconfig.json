{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src" /* Specify the base directory to resolve non-relative module names. */,
    "paths": {
      "~": ["./*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@enums/*": ["enums/*"],
      "@interface/*": ["interface/*"],
      "@images/*": ["images/*"],
      "@recoil/*": ["recoil/*"],
      "@stores/*": ["stores/*"],
      "@style/*": ["style/*"],
      "@pages/*": ["pages/*"]
    }
  }
}
